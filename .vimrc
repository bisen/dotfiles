colorscheme badwolf

set number
syntax on

" bosa yanmasin diye
set lazyredraw

set nocompatible
set hidden

set wildmenu
set hlsearch

set ignorecase
set smartcase

set ruler

set laststatus=2
set mouse=a

set cmdheight=2


set shiftwidth=4
set softtabstop=4
set expandtab

map Y y$

nnoremap <C-L> :nohl<CR><C-L>
